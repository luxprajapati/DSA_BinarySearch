class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
       int start = 0, end = nums.size() - 1;
        while (start<=end)
        {
            int  m = start+(end - start)/2;
            if(nums[m] == target)
            {
              return m;
            }
            else if(target > nums[m])
            {
               start = m + 1;   
            }
            
            else 
            {
                end = m-1;
            }
        }
       return start;  
    }
};
